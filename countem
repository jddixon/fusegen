#!/usr/bin/env bash

if [ ! -z "${DEV_BASE// }" ]
then
  cd $DEV_BASE/py/fusegen
  
  rm -rf build/* 
  
  pySloc -L py  -X cover -X htmlcov -X dist -X extern -v $@ fusegen src/fuse_decode src/run_fusegen tests/test_fuse_func.py tests/test_fuse_generation.py setup.py
else
  echo "DEV_BASE is not defined"
fi
